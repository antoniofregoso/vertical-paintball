<odoo>

<odoo>

    <menuitem id="paintball_management_menu" name="paintball Management" />

    <menuitem id="paintball_configuration_menu" name="Configuration"
        sequence="20" parent="paintball_management_menu" />
    <menuitem id="paintball_report_menu" name="Pdf Reports" sequence="6"
        parent="paintball_management_menu" />

    <record id="view_product_product_form_inherited" model="ir.ui.view">
        <field name="name">view.product.product.form.inherited</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='active']" position="after">
                <field name="isamenity" />
            </xpath>
        </field>
    </record>

  
 


    <!-- Form view of paintball amenity type -->
    <record id="view_paintball_amenity_type_form" model="ir.ui.view">
        <field name="name">paintball.amenity_type.form</field>
        <field name="model">paintball.amenity.type</field>
        <field name="arch" type="xml">
            <form string=" paintball amenity Type">
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="categ_id" select="1" />
                        <field name="child_ids" invisible="1" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Tree view of paintball amenity type -->
    <record id="view_paintball_amenity_type_tree" model="ir.ui.view">
        <field name="name">paintball.amenity_type.tree</field>
        <field name="model">paintball.amenity.type</field>
        <field name="arch" type="xml">
            <tree string="paintball amenity Type">
                <field name="name" />
            </tree>
        </field>
    </record>

    <!-- Action for paintball amenity type -->
    <record id="open_paintball_amenity_type_form_tree" model="ir.actions.act_window">
        <field name="name">amenity Type</field>
        <field name="res_model">paintball.amenity.type</field>
        <field name="view_type">form</field>
        <field name="context">{}</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_paintball_amenity" name="amenity"
        parent="paintball.paintball_configuration_menu" sequence="2" />
    <menuitem id="menu_open_paintball_amenity_type_form_tree" name="amenity Types"
        action="open_paintball_amenity_type_form_tree" sequence="6"
        parent="paintball.menu_paintball_amenity" />

  
    <!-- ====== amenitys ====== -->
    <!-- Form view of paintball amenity -->
    <record id="view_paintball_amenity_form" model="ir.ui.view">
        <field name="name">paintball.amenity.form</field>
        <field name="model">paintball.amenity</field>
        <field name="arch" type="xml">
            <form string="paintball amenity">
                <sheet>
                    <field name="image_medium" widget="image" class="oe_avatar" />
                    <div class="oe_title">
                        <label for="name" string="Name" />
                        <h1>
                            <field name="name" />
                        </h1>
                        <label for="status" string="Status" />
                        <h2>
                            <field name="status" readonly="1" />
                        </h2>
                        <h2>
                            <field name="isamenity" />
                        </h2>
                    </div>
                    <notebook>
                        <page name="information_paintball_amenity" string="Information">
                            <group colspan="4" col="4">
                                <field name="floor_id" string="Floor" />
                                <field name="categ_id" select="1" />
                                <field name="product_manager" select="2" string="amenity Manager" />
                                <field name="capacity" />
                                <field name="uom_id" invisible="1" />
                            </group>
                            <newline />
                            <separator colspan='4' string="Supplier Taxes" />
                            <field name="supplier_taxes_id" colspan="4" nolabel='1'
                                help='List of supplier taxes if there any on the defined amenity. ' />
                            <newline />
                            <separator colspan='4' string="Customer Taxes" />
                            <field name="taxes_id" colspan="4" nolabel='1'
                                help='Customer taxes apply on the perticular amenity. ' />
                        </page>
                        <page string="amenity Amenities">
                            <separator string=" amenity Amenities" />
                            <field name="amenity_amenities" colspan="4" nolabel="1" />
                        </page>
                        <page string="Procurement">
                            <group colspan="4" col="4">
                                <field name="active" select="2" />
                                <field name="list_price" />
                                <field name="rental" select="2" />
                                <field name="standard_price" />
                                <field name="cost_method" string="Cost Method" />
                                <newline />
                            </group>
                            <newline />
                            <group>
                                <separator string="Suppliers" />
                                <field name="seller_ids" colspan="4" nolabel="1"
                                    widget="one2many_list" />
                            </group>
                        </page>
                        <page string="Descriptions">
                            <separator string="Description" />
                            <field name="description" colspan="4" nolabel="1" />
                        </page>
                        <page string="Folio amenity Reservations">
                            <separator colspan='4' string="Folio amenity Reservation Line" />
                            <field name="amenity_line_ids" colspan="4" nolabel='1'>
                                <form string=" amenity line">
                                    <group colspan='4' col='4'>
                                        <field name="amenity_id" />
                                        <field name="check_in" />
                                        <field name="check_out" />
                                        <field name="folio_id" />
                                        <field name="status" />
                                    </group>
                                </form>
                                <tree>
                                    <field name="amenity_id" />
                                    <field name="check_in" />
                                    <field name="check_out" />
                                    <field name="folio_id" />
                                    <field name="status" />
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Search view of paintball amenity -->
    <record id="view_paintball_amenity_search" model="ir.ui.view">
        <field name="name">paintball.amenity.search</field>
        <field name="model">paintball.amenity</field>
        <field name="arch" type="xml">
            <search string="paintball amenity">
                <field name="name" filter_domain="[('name', 'ilike', self)]"/>
                <field name="categ_id" />
                <field name="list_price" string="amenity rate" />
                <field name="status" string="Status" />
                <newline />
                <filter name="available" string="Available" domain="[('status','=', 'available')]"/>
                <filter name="reserved" string="Reserved" domain="[('status','=', 'occupied')]"/>
            </search>
        </field>
    </record>

    <!-- Tree view of paintball amenity -->
    <record id="view_paintball_amenity_tree" model="ir.ui.view">
        <field name="name">paintball.amenity.tree</field>
        <field name="model">paintball.amenity</field>
        <field name="arch" type="xml">
            <tree string="paintball amenity">
                <field name="name" />
                <field name="categ_id" />
                <field name="list_price" string="amenity rate" />
                <field name="status" />
                <field name="capacity" />
            </tree>
        </field>
    </record>

    <record id="view_paintball_amenity_kanban" model="ir.ui.view">
        <field name="name">paintball.amenity.kanban</field>
        <field name="model">paintball.amenity</field>
        <field name="arch" type="xml">
            <kanban default_group_by="status">
                <field name="name" />
                <field name="status" />
                <field name="color" />
                <field name="lst_price" />
                <field name="id" />
                <field name="code" />
                <templates>
                    <t t-name="kanban-box">
                        <div
                            t-attf-class="oe_kanban_color_#{kanban_getcolor(record.color.raw_value)} oe_kanban_card oe_kanban_global_click">
                            <div class="o_dropdown_kanban dropdown" groups="base.group_user">
                                <a role="button" class="dropdown-toggle btn" data-toggle="dropdown" href="#">
                                    <span title="fa_icon" class="fa fa-bars fa-lg" />
                                </a>
                                <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                    <t t-if="widget.editable">
                                        <li>
                                            <a type="edit">Edit</a>
                                        </li>
                                    </t>
                                    <t t-if="widget.deletable">
                                        <li>
                                            <a type="delete">Delete</a>
                                        </li>
                                    </t>
                                    <li>
                                        <ul class="oe_kanban_colorpicker" data-field="color" />
                                    </li>
                                </ul>
                            </div>
                            <div class="oe_kanban_details" style='margin-top: -5px;'>
                                <div class="oe_kanban_content">
                                    <div class="oe_kanban_project_list">
                                        <H3>
                                            <a type="open">
                                                <img alt="amenity_image"
                                                    t-att-src="kanban_image('paintball.amenity', 'image_medium', record.id.value)"
                                                    style="height:70px;width:70px" class="oe_avatar oe_kanban_avatar_smallbox" />
                                                <t t-if="record.code.raw_value" style="margin-right: 10px">
                                                    [
                                                    <field name="code" />
                                                    ]
                                                </t>
                                                <field name="name" />
                                            </a>
                                        </H3>
                                    </div>
                                    <div name="tags" />
                                    <ul>
                                        <li>
                                            Status:
                                            <field name="status"></field>
                                        </li>
                                        <li>
                                            Rate:
                                            <field name="lst_price"></field>
                                        </li>
                                        <li>
                                            Capacity:
                                            <field name="capacity"></field>
                                        </li>
                                    </ul>
                                </div>
                                <div class="oe_kanban_bottom_right">
                                    <a t-if="record.status.raw_value === 'available'" type="object"
                                        string="Available" name="set_amenity_status_occupied"
                                        class="oe_kanban_status oe_kanban_status_green ">
                                    </a>
                                    <a t-if="record.status.raw_value === 'occupied'" type="object"
                                        string="occupied" name="set_amenity_status_available"
                                        class="oe_kanban_status oe_kanban_status_red ">
                                    </a>
                                </div>
                            </div>
                            <div class="oe_clear"></div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

    <!-- Action for paintball amenity -->
    <record id="action_paintball_amenity_form" model="ir.actions.act_window">
        <field name="name">amenity</field>
        <field name="res_model">paintball.amenity</field>
        <field name="view_type">form</field>
        <field name="context">{'default_isamenity':1,'default_rental':1,'default_available_in_pos':0}
        </field>
        <field name="view_id" ref="view_paintball_amenity_tree" />
        <field name="view_mode">kanban,tree,form</field>
    </record>

    <menuitem id="menu_open_paintball_amenity_form" name="amenitys"
        action="action_paintball_amenity_form" sequence="5" parent="paintball.menu_paintball_amenity" />

    <!-- Form view of paintball folio -->
    <record id="view_paintball_folio_form" model="ir.ui.view">
        <field name="name">paintball.folio.form</field>
        <field name="model">paintball.folio</field>
        <field name="arch" type="xml">
            <form string="paintball Folio">
                <header>
                    <button name="action_confirm" states="draft" string="Confirm Sale"
                        class="btn-primary" type="object" />
                    <button name="%(sale.action_view_sale_advance_payment_inv)d"
                        string="Create Invoice" type="action" class="btn-primary"
                        attrs="{'invisible': ['|',('state', 'in', ('draft','cancel','done')), ('invoice_status', 'in', 'invoiced')]}" />
                    <button name="action_cancel_draft" states="cancel" string="Set to Draft"
                        type="object" icon="fa-check-square-o" class="oe_highlight" />
                    <button name="action_cancel" string="Cancel Folio" states="sale"
                        type="object" icon="fa-close" class="oe_highlight" />
                    <button name="action_cancel" string="Cancel Folio" states="draft"
                        icon="fa-close" type="object" class="oe_highlight" />
                    <button name="action_done" type="object" string="Set to Done"
                        states="sale" class="oe_highlight"
                        help="If a paintball Folio is done, you cannot modify it manually anymore. However, you will still be able to invoice or deliver. This is used to freeze the paintball Folio." />
                    <field name="state" select="2" widget="statusbar"
                        statusbar_visible="draft,sent,sale,done" />
                </header>
                <sheet>
                    <label for="name" string="Folio Number" />
                    <h1>
                        <field name="name" colspan="4" />
                    </h1>
                    <group colspan="4" col="4">
                        <field name="date_order" readonly="1" />
                        <field name="warehouse_id" string="Branch" />
                        <field name="invoice_status" />
                        <field name="paintball_invoice_id" states='sale,done,cancel' readonly="1" />
                    </group>
                    <notebook>
                        <page name="folio" string="Folio">
                            <group colspan="4" col="4">
                                <field name="partner_id" string="Guest Name" required="1" />
                                <field name="partner_invoice_id" domain="[('parent_id','=',partner_id)]" />
                                <field name="pricelist_id" />
                                <field name="partner_shipping_id" domain="[('parent_id','=',partner_id)]" />
                            </group>
                            <newline />
                            <group colspan="4" col="4" invisible="1">
                                <field name="checkin_date" />
                                <field name="checkout_date" />
                                <field name="duration" readonly="1" />
                                <field name="duration_dummy" invisible="1" />
                            </group>
                            <separator string="amenity Lines" colspan="4" />
                            <field name="amenity_lines" colspan="4" string="amenity Line" nolabel="1"
                                context="{'checkin':checkin_date,'checkout':checkout_date,'folio':context.get('folio')}">
                                <form string="amenity Line">
                                    <notebook>
                                        <page name="folio_name" string="Folio Line">
                                            <group col="6" colspan="4">
                                                <field name="checkin_date" />
                                                <field name="checkout_date" />
                                                <separator string="Automatic Declaration" col="6"
                                                    colspan="4" />
                                                <field name="product_uom_qty"
                                                    context="{'partner_id':parent.partner_id,'quantity':product_uom_qty,'pricelist':parent.pricelist_id,'uom':product_uom}"
                                                    invisible="1" />
                                                <field name="product_id"
                                                    context="{'partner_id':parent.partner_id,'quantity':product_uom_qty,'pricelist':parent.pricelist_id,'uom':product_uom}"
                                                    domain="[('isamenity','=',True)]" string="amenity No" />
                                                <field name="product_uom" string="Rent(UOM)" />
                                            </group>
                                            <separator string="Manual Description" colspan="4" />
                                            <field name="name" colspan="4" select="2"
                                                placeholder="Description" />
                                            <group col="4" colspan="2">
                                                <field name="price_unit" select="2" string="Rent" />
                                                <field name="discount" />
                                                <newline />
                                                <field name="tax_id" colspan="4" nolabel="1" />
                                                <separator string="States" colspan="4" />
                                                <field name="state" select="2" />
                                                <field name="invoice_status" />
                                            </group>
                                        </page>

                                        <page name="history" string="History">
                                            <field name="invoice_lines" colspan="4" nolabel="1"
                                                readonly="1" />
                                        </page>
                                    </notebook>
                                </form>
                                <tree string="amenity Line">
                                    <field name="name" />
                                    <field name="checkin_date" />
                                    <field name="checkout_date" />
                                    <field name="product_id" string="amenity No" />
                                    <field name="product_uom" string="Rent(UOM)" />
                                    <field name="price_unit" string="Rent" />
                                    <field name="price_subtotal" />
                                    <field name="state" />
                                </tree>
                            </field>
                            <separator string="Service Lines" colspan="4" />
                            <field name="service_lines" colspan="4" string="Service Line"
                                nolabel="1" context="{'checkin':checkin_date,'checkout':checkout_date}">
                                <form name="service_line" string="Service Line">
                                    <notebook>
                                        <page name="service_line" string="Service Line">
                                            <group col="4" colspan="4">
                                                <field name="ser_checkin_date" />
                                                <field name="ser_checkout_date" />
                                            </group>
                                            <separator string="Automatic Declaration" col="4"
                                                colspan="4" />
                                            <group col="4" colspan="4">
                                                <field name="product_id" domain="[('isservice','=',True)]"
                                                    context="{'partner_id':parent.partner_id,'quantity':product_uom_qty,'pricelist':parent.pricelist_id,'paintball':parent.warehouse_id,'uom':product_uom}" />
                                                <field name="product_uom" />
                                                <field name="product_uom_qty"
                                                    context="{'partner_id':parent.partner_id,'quantity':product_uom_qty,'pricelist':parent.pricelist_id,'paintball':parent.warehouse_id,'uom':product_uom}"
                                                    invisible="1" />
                                            </group>
                                            <separator string="Manual Description" colspan="4" />
                                            <field name="name" colspan="4" select="2" placeholder="Description" />
                                            <group>
                                                <field name="price_unit" select="2" />
                                                <field name="discount" />
                                            </group>
                                            <newline />
                                            <field name="tax_id" colspan="4" />
                                            <group>
                                                <separator string="States" colspan="4" />
                                                <field name="state" select="2" />
                                                <field name="invoice_status" />
                                            </group>
                                        </page>

                                        <page name='invoice_history' string="History">
                                            <field name="invoice_lines" colspan="4" nolabel="1"
                                                readonly="1" />
                                        </page>
                                    </notebook>
                                </form>
                                <tree string="Service Line">
                                    <field name="name" />
                                    <field name="product_id" />
                                    <field name="product_uom_qty" />
                                    <field name="price_unit" />
                                    <field name="price_subtotal" />
                                    <field name="state" />
                                </tree>
                            </field>
                            <group colspan="2" class="oe_subtotal_footer oe_right">
                                <field name="amount_untaxed" sum="Untaxed amount" widget='monetary' />
                                <field name="amount_tax" widget='monetary' />
                                <div class="oe_subtotal_footer_separator oe_inline">
                                    <label for="amount_total" />
                                </div>
                                <field name="amount_total" nolabel="1" sum="Total amount"
                                    widget='monetary' />
                            </group>
                            <div class="oe_clear" />
                        </page>
                        <page name='other_data' string="Other data">
                            <group>
                                <field name="user_id" />
                                <field name="paintball_policy" attrs="{'readonly':[('state','not in',('draft'))]}" />
                                <field name="client_order_ref" />
                            </group>
                        </page>
                        <page name="invoice_history" string="History">
                            <separator string="Related invoices" colspan="4" />
                            <field name="invoice_ids" colspan="4" nolabel="1" />
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Tree view of paintball folio -->
    <record id="view_paintball_folio_tree" model="ir.ui.view">
        <field name="name">paintball.folio.tree</field>
        <field name="model">paintball.folio</field>
        <field name="arch" type="xml">
            <tree string="paintball Folio" colors="blue:state == 'draft';gray:state == 'done'">
                <field name="checkin_date" />
                <field name="checkout_date" />
                <field name="name" />
                <field name="partner_id" />
                <field name="date_order" />
                <field name="state" />
                <field name="amount_total" sum="Total amount" />
            </tree>
        </field>
    </record>

    <!-- Calendar view of paintball folio -->
    <record id="paintball_folio_calendar_view" model="ir.ui.view">
        <field name="name">paintball- Folios Calendar</field>
        <field name="model">paintball.folio</field>
        <field name="arch" type="xml">
            <calendar string="Folios" date_start="checkin_date" color="user_id"
                date_stop="checkout_date">
                <field name="name" />
                <field name="partner_id" />
                <field name="duration" />
            </calendar>
        </field>
    </record>

    <!-- Search view of paintball folio -->
    <record id="view_paintball_folio_search" model="ir.ui.view">
        <field name="name">paintball.folio.search</field>
        <field name="model">paintball.folio</field>
        <field name="arch" type="xml">
            <search string="Tables Detail">
                <field name="partner_id" />
                <field name="name" filter_domain="[('name', 'ilike', self)]" />
                <filter name="checkout_date" string="Current Booking"
                    domain="[('checkout_date','&gt;=',datetime.datetime.now().replace(hour=0, minute=0, second=0)),('checkin_date','&lt;=',datetime.datetime.now().replace(hour=23, minute=59, second=59))]"/>
                <group expand="0" string="Group By">
                    <filter name="date_order" string="Order By Month" context="{'group_by':'date_order'}" />
                    <filter name="partner_id" string="Customer" context="{'group_by':'partner_id'}" />
                </group>
            </search>
        </field>
    </record>

    <!--graph view of paintball folio -->
    <record id="view_paintball_folio_graph" model="ir.ui.view">
        <field name="name">view.paintball.folio.graph</field>
        <field name="model">paintball.folio</field>
        <field name="arch" type="xml">
            <graph type="bar">
                <field name="date_order" type="row" />
                <field name="amount_total" type="measure" />
            </graph>
        </field>
    </record>

    <!-- Action for paintball folio -->
    <record id="open_paintball_folio1_form_tree_all" model="ir.actions.act_window">
        <field name="name">Generate Folio</field>
        <field name="res_model">paintball.folio</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form,calendar,graph</field>
        <field name="context">{'folio': True}</field>
    </record>

    <menuitem id="menu_all_folio" name="Folio"
        parent="paintball.paintball_management_menu" sequence="4" />
    <menuitem name="Generate Folio" id="menu_open_paintball_folio1_form_tree_all"
        action="open_paintball_folio1_form_tree_all" sequence="5" parent="menu_all_folio" />

    <!--============ paintball Service Type ============ -->
    <!-- Form view of paintball service type -->
    <record id="view_paintball_service_type_form" model="ir.ui.view">
        <field name="name">paintball.service_type.form</field>
        <field name="model">paintball.service.type</field>
        <field name="arch" type="xml">
            <form string="Service Type">
                <sheet>
                    <group>
                        <field name="name" string="Service Name" />
                        <field name="service_id" />
                        <field name="child_ids" invisible="1" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Tree view of paintball service type -->
    <record id="view_paintball_service_type_tree" model="ir.ui.view">
        <field name="name">paintball.service_type.tree</field>
        <field name="model">paintball.service.type</field>
        <field name="arch" type="xml">
            <tree string="Service Type">
                <field name="name" />
            </tree>
        </field>
    </record>

    <!-- Action for paintball service type -->
    <record id="open_paintball_service_type_form_tree" model="ir.actions.act_window">
        <field name="name">Service Type</field>
        <field name="res_model">paintball.service.type</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_paintball_service" name="Services"
        parent="paintball.paintball_configuration_menu" sequence="2" />
    <menuitem name="Service Types" id="menu_open_paintball_service_type_form_tree"
        action="open_paintball_service_type_form_tree" sequence="9"
        parent="paintball.menu_paintball_service" />

    <!--============ paintball Service ============ -->
    <!-- Form view of paintball service -->
    <record id="view_paintball_services_form" model="ir.ui.view">
        <field name="name">.paintball.services.form</field>
        <field name="model">paintball.services</field>
        <field name="arch" type="xml">
            <form string="paintball Services">
                <sheet>
                    <label for="name" string="Name" />
                    <h1>
                        <field name="name" select="1" />
                    </h1>
                    <notebook>
                        <page string="Information">
                            <group>
                                <group colspan="4" col="4">
                                    <field name="type" select="2" string="Service Type" />
                                    <field name="categ_id" string="Service Category" />
                                    <field name="product_manager" select="2" string="Service Manager" />
                                    <field name="uom_id" />
                                    <field name="default_code" />
                                    <field name="uom_po_id" />
                                </group>
                            </group>
                            <newline />
                            <separator colspan='4' string="Supplier Taxes" />
                            <field name="supplier_taxes_id" colspan="4" nolabel='1'
                                help='List of supplier taxes related to the service provided by paintball.' />
                            <newline />
                            <separator colspan='4' string="Customer Taxes" />
                            <field name="taxes_id" colspan="4" nolabel='1'
                                help='Customer taxes applied on the service.' />
                        </page>
                        <page string="Procurement">
                            <group colspan="4" col="4">
                                <field name="active" select="2" />
                                <field name="list_price" />
                                <field name="cost_method" string="Cost Method" />
                                <field name="sale_ok" select="2" />
                                <field name="standard_price" />
                                <field name="rental" select="2" />
                            </group>
                            <newline />
                            <group>
                                <separator string="Suplliers" />
                                <field name="seller_ids" colspan="4" nolabel="1"
                                    widget="one2many_list" />
                            </group>
                        </page>
                        <page string="Descriptions">
                            <separator string="Description" />
                            <field name="description" colspan="4" nolabel="1" />
                            <separator string="Sale Description" />
                            <field name="description_sale" colspan="4" nolabel="1" />
                            <separator string="Purchase Description" />
                            <field name="description_purchase" colspan="4" nolabel="1" />
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Search view of paintball service -->
    <record id="view_paintball_services_search" model="ir.ui.view">
        <field name="name">paintball.services.search</field>
        <field name="model">paintball.services</field>
        <field name="arch" type="xml">
            <search string="paintball Services">
                <field name="name" />
                <field name="categ_id" string="Service Category" />
                <field name="list_price" string="Service rate" />
                <newline />
            </search>
        </field>
    </record>

    <!-- Tree view of paintball service -->
    <record id="view_paintball_services_tree" model="ir.ui.view">
        <field name="name">paintball.services.tree</field>
        <field name="model">paintball.services</field>
        <field name="arch" type="xml">
            <tree string="paintball Services">
                <field name="name" />
                <field name="categ_id" string="Service Category" />
                <field name="list_price" string="Service rate" />
            </tree>
        </field>
    </record>

    <!-- Action for paintball service -->
    <record id="action_paintball_services_form" model="ir.actions.act_window">
        <field name="name">Services</field>
        <field name="res_model">paintball.services</field>
        <field name="view_type">form</field>
        <field name="context">{'default_isservice':1,'default_available_in_pos':0}
        </field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem name="Services" id="menu_open_paintball_services_form"
        action="action_paintball_services_form" sequence="8"
        parent="paintball.menu_paintball_service" />

</odoo>
 
</odoo>